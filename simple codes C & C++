// TO CHECK WHETHER A NUMBER IS A PERFECT SQUARE

#include <stdio.h>
int main()
{
  int i,n,flag=0;
  scanf("%d",&n);
  for(i=0;i<n;i++)
  {
    if(n==i*i)
    flag=1;
  }
  if(flag==1)
  printf("TRUE");
  else
  printf("FALSE");
	return 0;
}

// FIBONACCI SERIES

#include <stdio.h>
int fib(int n)
{
  int i,c;
  int a=0;
  int b=1;
  for(i=1;i<=n;i++)
  {
    printf("%d ",a);
    c=a+b;
    a=b;
    b=c;
  }
}
int main() 
{
  int n;
  scanf("%d",&n);
  fib(n);
	return 0;
}

// TO REVERSE AN ARRAY

#include <bits/stdc++.h>
using namespace std;
int main()
{
  int i,a[10000],n;
  cin>>n;
  for(i=1;i<=n;i++)
  cin>>a[i];
  for(i=n;i>0;i--)
  cout<<a[i]<<" ";
  return 0;
}
 
 // to find max of 3 integers (shortest method)

#include <iostream>
using namespace std;
int main()
{
	int a,b,c;
	cin>>a>>b>>c;
	int max=(a>b)&&(a>c)?a:(b>c)?b:c;
	cout<<max;
	return 0;
}

// TO FIND THE SUM OF AN ARRAY

#include <stdio.h>

int main()
{
    int i,n;
    int sum=0;
    int a[100];
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    for(i=0;i<n;i++)
    {
        sum=sum+a[i];
    }
    
    printf("%d",sum);
    return 0;
}
