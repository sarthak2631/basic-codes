// TO CHECK WHETHER A NUMBER IS PRIME OR NOT

#include <stdio.h>
void prime(int n)
{
  int i,f=0;
  for(i=2;i<=n/2;i++)
  {
      if(n%i==0)
      {
          f=1;
          break;
      }
  }
  if(n==1)
  {
    printf("Not Prime");
  }
    else
    {
      if(f==0)
        printf("Prime");
      else
        printf("Not Prime");
    }
}
int main()
{
  int n;
  scanf("%d",&n);
  prime(n);
	return 0;
}


// TO CHECK WHETHER A NUMBER IS A PERFECT SQUARE

#include <stdio.h>
int main()
{
  int i,n,flag=0;
  scanf("%d",&n);
  for(i=0;i<n;i++)
  {
    if(n==i*i)
    flag=1;
  }
  if(flag==1)
  printf("TRUE");
  else
  printf("FALSE");
	return 0;
}

// FIBONACCI SERIES

#include <stdio.h>
int fib(int n)
{
  int i,c;
  int a=0;
  int b=1;
  for(i=1;i<=n;i++)
  {
    printf("%d ",a);
    c=a+b;
    a=b;
    b=c;
  }
}
int main() 
{
  int n;
  scanf("%d",&n);
  fib(n);
	return 0;
}

// FACTORIAL OF A NUMBER

#include <stdio.h>
int fact(int n)
{
    if(n==0)
    return 1;
    else
    return (n*fact(n-1));
}
int main()
{
  int n,r;
  scanf("%d",&n);
  r=fact(n);
  printf("%d",r);
	return 0;
}

// TO REVERSE AN ARRAY

#include <bits/stdc++.h>
using namespace std;
int main()
{
  int i,a[10000],n;
  cin>>n;
  for(i=1;i<=n;i++)
  cin>>a[i];
  for(i=n;i>0;i--)
  cout<<a[i]<<" ";
  return 0;
}
 
 // to find max of 3 integers (shortest method)

#include <iostream>
using namespace std;
int main()
{
	int a,b,c;
	cin>>a>>b>>c;
	int max=(a>b)&&(a>c)?a:(b>c)?b:c;
	cout<<max;
	return 0;
}

// TO FIND THE SUM OF AN ARRAY

#include <stdio.h>

int main()
{
    int i,n;
    int sum=0;
    int a[100];
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    for(i=0;i<n;i++)
    {
        sum=sum+a[i];
    }
    
    printf("%d",sum);
    return 0;
}


// TO FIND WHETHER A NUMBER IS ARMSTRONG NUMBER OR NOT

#include <stdio.h>
int main()
{
  int n,rem,d,r=0;
  scanf("%d",&n);
  d=n;
  while(d!=0)
  {
    rem=d%10;
    r=r + (rem*rem*rem);
    d=d/10;
  }
  
  if(r==n)
    printf("Armstrong Number");
  else
    printf("NOT Armstrong Number");
	return 0;
}
